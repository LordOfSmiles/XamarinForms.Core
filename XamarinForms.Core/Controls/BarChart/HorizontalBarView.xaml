<?xml version="1.0"
      encoding="utf-8"?>
<barChart:BarWidgetBase xmlns="http://xamarin.com/schemas/2014/forms"
                      xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                      xmlns:barChart="clr-namespace:XamarinForms.Core.Controls.BarChart;assembly=XamarinForms.Core"
                      x:Class="XamarinForms.Core.Controls.BarChart.HorizontalBarView">
    <Grid ColumnSpacing="0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition x:Name="columnFilled" />
            <ColumnDefinition x:Name="columnEmpty" />
        </Grid.ColumnDefinitions>

        <BoxView x:Name="bxValue"
                 Grid.Column="0" />
    </Grid>
</barChart:BarWidgetBase>